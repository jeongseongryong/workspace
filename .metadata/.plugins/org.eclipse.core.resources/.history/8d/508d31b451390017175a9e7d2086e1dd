package ppp;

 class sss {
	 public static final int DEFAULT_SIZE = 10;
	 public static final int GROW_SIZE = 1000;
	 
	 private Object[] list;
	 private int length;
	 
	 public sss() {
		 list = new Object[DEFAULT_SIZE];
	 }
	 public sss(int capacity) {
		if(capacity < DEFAULT_SIZE || capacity > GROW_SIZE) {
			list = new Object[10];
		} else {
			list = new Object[capacity];
			
		}
	 }
 }