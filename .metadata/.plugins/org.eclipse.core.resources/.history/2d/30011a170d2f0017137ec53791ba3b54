/*변수 선언 :배얄 변수와 초기화 */
		package step02;

public class Test7 {
	public static void main(String[] args) {

      byte[] arr1 = new byte[3];
      
      
     arr1[0] = 10;
     arr1[1] = 20;
     arr1[2] = 30;
     
     System.out.printf("%d,%d,%d\n",arr1[0],arr1[1],arr1[2]);
     
     
   }
}
    
