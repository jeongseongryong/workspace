/* 데이터 프로세싱 스트림 클래스 - ObjectOutputStream 도입 후
	=> "직렬화(Serializable)" 인스턴스를 바이트 배열로 바꿈 
 */
package step16;

import java.io.FileOutputStream;
import java.io.ObjectOutputStream;


public class Test05_2_out {
	
		

	public static void main(String[] args) throws Exception {
		Student s = new Student ("홍길동", 100, 90, 80, false);
		
		FileOutputStream out0 = new FileOutputStream("temp/Test05_1.data");
		ObjectOutputStream out = new ObjectOutputStream(out0);
		
		//객체 출력
		out.writeObject(s);
		
		out.close();
		out0.close();
	}
		
		
}


