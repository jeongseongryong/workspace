/* 논리연산자 - (&&, ||, !)
 * 
 */
package step03;

public class Test03_2 {
  public static void m1(int value) {
  	System.out.println(value);
  }
  public static void main(String[] args) {
    System.out.println(true && true);
    System.out.println(true && false);
    System.out.println(true && true);
    System.out.println(true && false);
    System.out.println(true && true);
    System.out.println(true && true);
  	
  }
}