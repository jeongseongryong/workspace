/* 인스턴스 변수와 인스턴스 메서드
 *
 */
package step07;

public class Test04_1 {
  static class Calculator {
     int result;
     
     public void plus(int value) {
    	 this.result += value;
     }
     public void minus(int value) {
    	 this.result -= value;
     }
  }
  
  public static void main(String[] args) {
  	Student.name = "하하하";
  	System.out.println(Student.name);

  }
}
