package step09.ex1;

public class Test03 {
	public static void main(String[] args) {
		MasterCalculator3 calc = new MasterCalculator3();
		calc.plus(100);
		calc.plus(20);
		calc.minus(30);
		calc.multiple(2);
		calc.divide(3);
		System.out.println(calc.result);
	}
}

