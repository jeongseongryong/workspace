/* character stream 클래스 : FileWriter vs FileInputStream
 */
package step16;

import java.io.FileOutputStream;


public class Test06_1_out {
	
		

	public static void main(String[] args) throws Exception {
		Student s = new Student ("홍길동", 100, 90, 80, false);
		FileOutputStream out0 = new FileOutputStream("temp/Test06_1.data");
		
			
		out.writeUTF(s.name);
		out.writeInt(s.kor);
		out.writeInt(s.eng);
		out.writeInt(s.math);
		out.writeInt(s.sum);
		out.writeFloat(s.aver);
		out.writeBoolean(s.working);
		
		
		out.close();
		out0.close();
	}
		
		
}


