/*다형성 - 다형적 변수와 instanceof 연산자
 *
 */
package step09.ex4;

public class Test04 {

	public static void main(String[] args) {
	
		Member m1 = new Member();
		Member m2 = new Student();
		Member m3 = new Manager();
		Member m4 = new Teacher();
		Member m5 = new FreeStudent();
	}
	
	static void checkReference(Member m) {
		if(m instanceof Member)System.out.println("Member의 인스턴스 이다.");
		if(m instanceof Student)System.out.println("Member의 인스턴스 이다.");
		if(m instanceof Manager)System.out.println("Member의 인스턴스 이다.");
		if(m instanceof Teacher)System.out.println("Member의 인스턴스 이다.");
		if(m instanceof FreeStudent)System.out.println("Member의 인스턴스 이다.");
		if(m instanceof Member)System.out.println("Member의 인스턴스 이다.");
		
	}

}
