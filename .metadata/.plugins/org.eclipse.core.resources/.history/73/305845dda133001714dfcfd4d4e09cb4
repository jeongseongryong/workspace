/* 문장:조건문 switch- break */
package step04;

public class test02_3 {
  public static void main(String[] args) {
   byte b = 10;
   short s = 10;
   int i = 10;
   long l = 10;
   float f = 10;
   double d = 10;
   boolean bool = true;
   char c = '가';
   
   // 4바이트 크기의 정수 값 가능!
   switch (b) {}
   switch (s) {}
   switch (i) {}
//   switch (l) {}
   switch (c) {}
   
   // 문자영 (java 1.7 부터 가능)
 }
}
