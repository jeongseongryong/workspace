/* nested class: non-static nested class
 * "inner class" 라고도 한다.
 * 인스턴스 멤버이기 때문에 다른 인스턴스 멤버만이 사용 가능
 * 
 */
package step13;



public class Test03_1 {
	
	class A {
		int a;
	}
	
	public void test() {
		A obj = new A();
		obj.a = 200;
	}
	
	public static void main(String[] args) {
		A obj = new A();
	}
}
