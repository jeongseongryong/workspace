/* 문장:반복문  for */
package step04;

public class test03_8 {
   
  public static void main(String[] args) {
   for (int i = 2; i <=9; i++) {
  	 System.out.println(i + ",");
   }
    System.out.println("------------------");
    
    int i = 2;
    for (; i <=9; i++) {
   	 System.out.println(i);
    }
    i = 2;
     System.out.println("------------------");
 }
}
