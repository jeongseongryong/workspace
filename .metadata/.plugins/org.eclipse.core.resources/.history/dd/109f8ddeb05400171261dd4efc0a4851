/* 로그아웃 서블릿: */
package bitcamp.java93.control;

import java.io.IOException;

import javax.servlet.ServletException;
import javax.servlet.annotation.WebServlet;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;


public class LogoutControl implements Controller  {


  @Override
  public String service(HttpServletRequest req, HttpServletResponse res) throws ServletException, IOException {
  	 req.getSession().invalidate(); // 현재 요청한 클라이언트 전용 HttpSession 객체를 무효화시킴
  	 
     return "../auth/login";   
  }  
}