package ppp;

public class sss4 {
	public static void main(String[] args) {
		int a = 10;
		int b = 20;
		char c = '*';
		
		switch (c) {
		case '+':
			System.out.println(a + b);
		case '-':
			System.out.println(a - b);
		case '*':
			System.out.println(a * b);
		case '/':
			System.out.println(a / b);
		case '%':
			System.out.println(a % b);
			default:
				System.out.println("지원 ㄴㄴ");
		}
	}
}