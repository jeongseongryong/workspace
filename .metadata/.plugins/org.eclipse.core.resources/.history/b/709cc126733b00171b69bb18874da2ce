 * 
 */
package step17;

public class Test11 {	
	static void m1(int v) {
		
	}
	
	static void m2(int v) {
		
	}

	static void m3(int v) {
	
	}

	static void m4(int v) throws Exception {
		
			if(v < 0)
				throw new Exception("음수는 허용하지않습니다.");
			System.out.println(v);
			
		
	}
	public static void main(String[] args) {
		
		
	
	}
		
}


