package step08;

import java.sql.Date;

public class Test06_1  {
	
	public static void main(String[] args){
		Date[] dateList = new Object[3];
		
		dateList[0] = Date.valueOf("2017-3-1");
		dateList[1] = Date.valueOf("2017-5-3");
		dateList[2] = Date.valueOf("2017-5-5");
		
		Date[] temp = new Date[6];
		for (int i = 0; i < dateList.length; i++) {
			copyArray(dateList, temp);
			
			dateList = temp;
		}
	}
	public static Object growArray(Object[] arr, Object[] target) {
		Object [] newArr = new Object[arr.length + size];
		for(int i = 0; i < arr.length; i++) {
			arr[i] = arr[i];
		}
	}
}
	
	


