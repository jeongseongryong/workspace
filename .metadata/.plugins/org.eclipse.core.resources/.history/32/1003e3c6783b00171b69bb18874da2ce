/*예외처리 :finally 블록
 * 
 */
package step17;

public class Test13 {	

	
	public static void main(String[] args) {
		try {
			if (args.length < 1)
				throw new Exception("한 개의 숫자를 붙이시오!");
			
			System.out.println(Integer.parseInt(args[0]));
			
		}catch (Exception e) {
			e.printStackTrace();
			
		} finally {
			
			System.out.println("오호라~~~");
		}
		System.out.println("안녕!");
	}
		
}


