/* nested class: non-static nested class
 * "inner class" 라고도 한다.
 * 인스턴스 멤버이기 때문에 다른 인스턴스 멤버만이 사용 가능
 * 
 */
package step13;


// package member class
public class Test03_1 {
	
	//static variable
	static int v1;
	
	//static method
	static void m1() {}
	
	// static nested class
	static class A  { 
		int a;
	}
	
	public static void main(String[] args) {
		v1 = 10;
		m1();
		A obj = new A();
	}

	public void test() {
		v1 = 20;
		
		m1();
		
		A obj = new A(); 
	}
}
