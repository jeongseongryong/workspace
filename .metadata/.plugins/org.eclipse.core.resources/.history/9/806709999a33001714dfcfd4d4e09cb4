/* 비트 이동 연산자 (>>, >>>, <<)
 * 
 */
package step03;

public class Test03_6 {
  public static void main(String[] args) {
    int a = 0b0000_1011;
    
    System.out.println(a);
    System.out.println(a >> 1); //a / 2^1
    System.out.println(a >> 2); //a / 2^2
    System.out.println(a >> 3); //a / 2^3
  }
}