/*다형성 - 다형적 변수 II
 * =>어떤 레퍼런스 변수는 그타입의 인스턴스 뿐만아니라,
 * 	  그 타입의 서브 클래스의 인스턴스까지 저장할 수 있다.
 */
package step09.ex4;

public class Test02 {

	public static void main(String[] args) {
		// Member m = new Member(); //ok
		// Member m = new Student(); // ok 
		Member m = new FreeStudent();
		m.name = "홍길동";
		m.id = "hong";
		m.password = "1111";
		m.tel = "1111-1111";
		m.email = "hong@test.com";
		
	}

}
